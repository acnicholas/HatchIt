<?xml version="1.0" encoding="utf-8"?>
<Window
    x:Class="Hatchit.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Hatchit"
    Height="300"
    Width="300">
    <Grid>

            <ItemsControl ItemsSource="{Binding Lines}">
            <ItemsControl.ItemsPanel>
            <ItemsPanelTemplate>
            <Canvas
            Width="Auto"
            Height="Auto"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Margin="0"
            Grid.Column="0"
            Grid.Row="0"
            x:Name="MyCanvas" />
            </ItemsPanelTemplate>
            </ItemsControl.ItemsPanel>
        </ItemsControl>

        <Button Command="{Binding ClickCommand}" Width="100" Height="100" Content="wefwfwef"/>

    </Grid>
</Window>